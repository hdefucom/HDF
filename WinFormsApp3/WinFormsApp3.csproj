<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net48</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<LangVersion>10.0</LangVersion>

		<!--没有指定平台，WebView2无法初始化成功-->
		<!--<PlatformTarget>AnyCPU</PlatformTarget>-->

		<PlatformTarget>x86</PlatformTarget>

		<!--<CefSharpAnyCpuSupport>true</CefSharpAnyCpuSupport>-->

		<GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>


		<!--<GeneratePackageOnBuild>true</GeneratePackageOnBuild>-->
		<VersionPrefix>1.1.1</VersionPrefix>

		
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\WinFormsApp1\新文件夹\*.cs">
			<Link>new\%(Filename).cs</Link>
		</Compile>


		<AssemblyAttribute Include="System.CLSCompliantAttribute">
			<_Parameter1>true</_Parameter1>
		</AssemblyAttribute>

	</ItemGroup>



	<!--<ItemGroup>
		<Compile Include="..\WinFormsApp1\新文件夹\*.cs" Link="new\%(Filename).cs" />
		<Compile Remove="new\Class1.cs" />
	</ItemGroup>-->



	<!--<ItemGroup>
	  <Compile Include="..\WinFormsApp1\新文件夹\Class1.cs" Link="Class1.cs" />
	  <Compile Include="..\WinFormsApp1\新文件夹\Class2.cs" Link="Class2.cs" />
	</ItemGroup>-->


