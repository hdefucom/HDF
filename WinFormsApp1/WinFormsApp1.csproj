<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net40</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<PlatformTarget>AnyCPU</PlatformTarget>

		<LangVersion>10.0</LangVersion>
		<AutoGenerateBindingRedirects>false</AutoGenerateBindingRedirects>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>

		
		<Deterministic>false</Deterministic>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="HDF.Common" Version="1.3.0" />
		<PackageReference Include="Microsoft.Bcl.Async" Version="1.0.168" />
		<PackageReference Include="Syncfusion.Edit.Windows" Version="19.3.0.54" />
		<PackageReference Include="System.ValueTuple" Version="4.5.0" />
		<PackageReference Include="TinyMapper" Version="3.0.3" />
	</ItemGroup>


	<ItemGroup>
		<Reference Include="System.Configuration" />
		<Reference Include="System.Design">
		  <HintPath>C:\Program Files (x86)\Reference Assemblies\Microsoft\Framework\.NETFramework\v4.7.2\System.Design.dll</HintPath>
		</Reference>
		<Reference Include="System.Management" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Properties\Resources.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Resources.resx</DependentUpon>
		</Compile>
		<EmbeddedResource Update="Properties\Resources.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>Resources.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

	<Import Project="..\Common.Config.props" Condition="Exists('..\Common.Config.props')" />


</Project>