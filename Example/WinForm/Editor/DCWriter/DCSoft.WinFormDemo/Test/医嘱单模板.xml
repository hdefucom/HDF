   <ScriptText>sub Document_EventAfterPaintElement( sender as object , args as ElementPaintEventArgs)
	if TypeOf args.Element is XTextTableRowElement then
		dim txt as string = args.Element.GetAttribute("取消状态")
		if txt &lt;&gt; "" then
			' 绘制取消状态文本 
			dim row as XTextTableRowElement = args.Element
			dim format as new StringFormat()
			format.Alignment = StringAlignment.Far
			format.LineAlignment = StringAlignment.Far
			dim font as new XFontValue("宋体" , 9 , FontStyle.Bold )
			dim rect as RectangleF = args.ClientViewBounds
			rect.X += row.Cells(2).Left
			rect.Width = row.Cells(2).Width
			args.Graphics.DrawString(txt , font.Value, Brushes.Red, rect , format)
			format.Dispose()
		end if
	end if
end sub</ScriptText>
